generator client{
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String 
  email String @unique
  Password String
  CreatedAt DateTime @default(now())
}

model Task{
  id String @id @default(uuid())
  Title String
  Description String
  
}